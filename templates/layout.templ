package templates

templ Layout(title string) {
	<!DOCTYPE html>
	<html lang="en">
		<head>
			<meta charset="UTF-8"/>
			<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
			<title>{ title }</title>
			<link href="/static/css/styles.css" rel="stylesheet"/>
		</head>
		<body class="bg-[#0F1322]">
			@Navbar()
			{ children... }
		</body>
	</html>
}
